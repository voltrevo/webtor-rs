{
  "name": "webtor-rs",
  "version": "0.1.0",
  "description": "Tor client implementation in Rust, compiled to WebAssembly for web browsers",
  "scripts": {
    "build": "./build.sh",
    "build:wasm": "cd webtor-wasm && wasm-pack build --target web && cd ../webtor-demo && wasm-pack build --target web",
    "serve": "cd webtor-demo/static && python3 -m http.server 8000",
    "serve:node": "cd webtor-demo/static && npx serve -s . -p 8000",
    "test": "cargo test --workspace",
    "test:wasm": "wasm-pack test --headless --firefox",
    "test:wasm:tls12": "cd subtle-tls && wasm-pack test --headless --chrome --features tls12",
    "test:headless": "node test-headless.mjs",
    "test:headed": "node test-headless.mjs --headed",
    "test:tls": "node tests/e2e/test-tls-connections.mjs",
    "test:tls:headed": "node tests/e2e/test-tls-connections.mjs --headed",
    "test:tls:debug": "node tests/e2e/test-tls-connections.mjs --headed --debug",
    "test:tls12": "node tests/e2e/test-tls12-servers.mjs",
    "test:tls12:headed": "node tests/e2e/test-tls12-servers.mjs --headed",
    "test:bench": "node tests/e2e/test-benchmark.mjs --quick",
    "test:bench:full": "node tests/e2e/test-benchmark.mjs",
    "test:bench:headed": "node tests/e2e/test-benchmark.mjs --headed --quick",
    "clean": "cargo clean && rm -rf webtor-wasm/pkg webtor-demo/pkg",
    "dev": "cargo watch -s 'npm run build:wasm'"
  },
  "devDependencies": {
    "serve": "^14.2.0",
    "playwright": "^1.40.0"
  },
  "keywords": [
    "tor",
    "privacy",
    "anonymity",
    "rust",
    "webassembly",
    "wasm",
    "snowflake",
    "proxy",
    "browser"
  ],
  "author": "Webtor Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voltrevo/webtor-rs.git"
  },
  "bugs": {
    "url": "https://github.com/voltrevo/webtor-rs/issues"
  },
  "homepage": "https://github.com/voltrevo/webtor-rs#readme"
}